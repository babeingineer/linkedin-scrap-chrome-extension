{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [
        "https://www.linkedin.com/*"
    ]
  }